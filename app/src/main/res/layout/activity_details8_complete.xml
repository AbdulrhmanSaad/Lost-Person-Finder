<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailsCompleteActivity8">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >


    <TextView
        android:id="@+id/textView18"
        android:layout_width="364dp"
        android:layout_height="143dp"
        android:layout_marginTop="19dp"
        android:fontFamily="@font/proxima_nova_semibold"
        android:gravity="center"
        android:text="Your information has been successfully submitted; we will review it and get back to you shortly."
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/Btn_completeGoToDashboard"
        android:layout_width="363dp"
        android:layout_height="72dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/btn_background"
        android:backgroundTint="@color/app_blue"
        android:fontFamily="@font/proxima_nova_semibold"
        android:text="Back to Dashboard"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.479"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.09" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="363dp"
        android:layout_height="249dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/img_complete_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.479"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>